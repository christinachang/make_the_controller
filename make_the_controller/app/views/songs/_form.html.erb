<%= form_for(@song) do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <select name="song[artist_id]">
    <%Artist.all.each do |a| %>
  <option value="<%=a.id%>"><%=a.name%></option>
  <%end%>
  </select>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
